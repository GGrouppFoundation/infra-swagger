<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net6.0;net7.0</TargetFrameworks>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <InvariantGlobalization>true</InvariantGlobalization>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <RootNamespace>GGroupp.Infra</RootNamespace>
    <AssemblyName>GGroupp.Infra.Swagger.Builder</AssemblyName>
    <Version>3.4.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="GGroupp.Infra.Swagger.Builder.Abstractions" Version="2.2.1" />
    <PackageReference Include="GGroupp.Infra.Swagger.Core" Version="3.3.0" />
    <PackageReference Include="GGroupp.Infra.Swagger.Option" Version="2.3.1" />
  </ItemGroup>

</Project>