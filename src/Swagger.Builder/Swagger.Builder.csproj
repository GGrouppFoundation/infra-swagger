<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <InvariantGlobalization>true</InvariantGlobalization>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <RootNamespace>GGroupp.Infra</RootNamespace>
    <AssemblyName>GGroupp.Infra.Swagger.Builder</AssemblyName>
    <Version>3.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="GGroupp.Infra.Swagger.Builder.Abstractions" Version="2.0.0" />
    <PackageReference Include="GGroupp.Infra.Swagger.Core" Version="3.0.0" />
    <PackageReference Include="GGroupp.Infra.Swagger.Option" Version="2.0.0" />
  </ItemGroup>

</Project>